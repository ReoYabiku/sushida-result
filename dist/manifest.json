{
    "manifest_version": 3,
    "name": "sushida result",
    "description": "sushida result to google spread sheet",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": ["activeTab", "scripting"],
    "content_security_policy": {
      "extension_pages": "default-src 'self'; script-src-elem 'self' https://cdn.jsdelivr.net;"
    }
}
